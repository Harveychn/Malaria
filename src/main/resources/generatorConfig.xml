               enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true">
            <generatedKey column="category_id2" sqlStatement="JDBC"></generatedKey>
        </table>

        <table tableName="disease" domainObjectName="Disease"
               enableCountByExample="true" enableUpdateByExample="true"
               enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true">
            <generatedKey column="disease_id" sqlStatement="JDBC"></generatedKey>
            
        </table>

        <table tableName="doctor" domainObjectName="Doctor"
               enableCountByExample="true" enableUpdateByExample="true"
               enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true">
            <generatedKey column="doctor_id" sqlStatement="JDBC"></generatedKey>
        </table>

        <table tableName="four_level_linkage" domainObjectName="FourLevelLinkage"
               enableCountByExample="true" enableUpdateByExample="true"
               enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true">
            <generatedKey column="id" sqlStatement="JDBC"></generatedKey>
        </table>

        <table tableName="indicator" domainObjectName="Indicator"
               enableCountByExample="true" enableUpdateByExample="true"
               enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true">
        </table>

        <table tableName="medical_unit" domainObjectName="MedicalUnit"
               enableCountByExample="true" enableUpdateByExample="true"
               enableDeleteByExample="true" enableSelectByExample="true"
